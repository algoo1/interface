{
    "builds": [
        {
            "src": "main.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "main.py"
        },
        {
            "src": "/",
            "dest": "/static/index.html"
        },
        {
            "src": "/(.*)",
            "dest": "/static/$1"
        }
    ]
}